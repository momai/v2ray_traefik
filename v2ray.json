      "label": "Vpn | v2ray-websocket_2 | momai"
    },
    {
      "listen": "0.0.0.0",
      "port": 8390,
      "protocol": "shadowsocks",
      "settings": {
        "password": "password",
        "method": "aes-256-gcm"
      },
      "streamSettings": {
